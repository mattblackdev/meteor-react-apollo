    "apollo-client": "^2.5.0",
    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "apollo-link-ws": "^1.0.14",
    "apollo-live-client": "^0.2.1",
    "apollo-live-server": "^0.2.0",
    "apollo-morpher": "^0.2.0",
    "apollo-server-express": "^2.4.8",
    "apollo-upload-client": "^10.0.0",
    "bcrypt": "^3.0.4",
